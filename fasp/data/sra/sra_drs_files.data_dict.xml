<?xml-stylesheet type="text/xsl" href="./datadict_v2.xsl"?>
<data_table id="sra_drs_files" study_id="onek:001" 
    date_created="Sun Sep 20 09:49:50 2020">
    <description>List of thousand genome DRS ids for recalibrated variant calls</description>
    <variable id="sra_drs_files.var1">
        <name>acc</name>
        <description>SRA run accession no</description>
        <type>string</type>
    </variable>
    <variable id="sra_drs_files.var2">
        <name>filename</name>
        <description>Name of file in SRA</description>
        <type>string</type>
    </variable>
    <variable id="sra_drs_files.var3">
        <name>drs_id</name>
        <description>local DRS id unique within the DRS service</description>
        <type>string</type>
    </variable>
    <variable id="sra_drs_files.var4">
        <name>filetype</name>
        <description>Type of file</description>
        <type>encoded value</type>
        <value>bam</value>
        <value>bai</value>
    </variable>
    <variable id="sra_drs_files.var5">
        <name>sample_name</name>
        <description>1000 genomes project sample name</description>
        <type>string</type>
    </variable>
    <variable id="sra_drs_files.var6">
        <name>mapped</name>
        <description>Indicates whether the file contains reads which could be mapped to the reference genome, or reads which could not (unmapped). See https://www.internationalgenome.org/faq/what-are-unmapped-bams/</description>
        <type>encoded value</type>
        <value>mapped</value>
        <value>unmapped</value>
    </variable>
    <variable id="sra_drs_files.var7">
        <name>sequencing_type</name>
        <description>Type of sequencing</description>
        <type>encoded value</type>
        <value>exome</value>
        <value>low_coverage</value>
    </variable>
</data_table>